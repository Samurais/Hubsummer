	.topNav {
		position: fixed;
		top: 26px;
		z-index: 1;
		font-size: 14px;
		height: 36px;
		line-height: 36px;
		border-bottom: 1px solid #d7d7d7;
		background-color: #eee;
		font-family: "Microsoft YaHei", SimSun;
		width: 100%;
	}
	
	.topNav-width {
		margin: 0 auto;
		padding: 0;
		height: 36px;
	}
	
	.topNav h3 {
		font-weight: normal;
		font-size: 100%;
	}
	
	.topNav dl {
		zoom: 1;
	}
	
	.topNav .tnLeft {
		float: left;
		margin-top: -10px;
		height: 36px;
	}
	
	.topNav .tnRight {
		float: right;
	}
	.topNav dd {
		float: left;
		padding: 0;
		margin: 0;
		height: 36px;
		position: relative;
	}
	
	.topNav dd h3 {
		float: left;
		line-height: 36px;
	}
	
	.topNav dd h3 a {
		text-decoration: none;
		color: #333;
		display: inline-block;
		*display: inline;
		zoom: 1;
		height: 36px;
		line-height: 36px;
		padding: 0px 7px;
	}
	
	
	
	.topNav dd ul {
		list-style: none;
		display: none;
		position: absolute;
		width: 110px;
		top: 45px;
		margin-left: 0;
		/*left: 0;*/
		border: 1px solid #d7d7d7;
		border-top: 0;
		background: #fff;
		-moz-box-shadow: 3px 3px 3px rgba(0, 0, 0, .1);
		-webkit-box-shadow: 3px 3px 3px rgba(0, 0, 0, .1);
		box-shadow: 3px 3px 3px rgba(0, 0, 0, .1);
	}
	
	.topNav dd ul li {
		line-height: 31px;
	}
	
	.topNav dd ul li a {
		text-decoration: none;
		display: block;
		color: #333;
		padding: 0 15px;
	}
	
	
	
