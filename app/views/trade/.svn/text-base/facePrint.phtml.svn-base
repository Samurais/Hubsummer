/**********获取订单当前用户名和订单总数*************/
function gettidnum(){
	QN.top.invoke({
        "cmd": "taobao.user.seller.get",
        "param":{
            fields:'nick,type'
        },
        "method": "get",
        "success" : function(rsp) {
            if(typeof rsp === 'string'){
                var rsp = JSON.parse(rsp);
            }else{
                var	rsp= rsp;
            }
            if(rsp.code!=null){
                geterrorinfo('getsellernick-rsp',JSON.stringify(rsp));
            }else{
                sellernick=rsp.user_seller_get_response.user.nick;
                if(rsp.user_seller_get_response.user.type=='B')sellertype='TM';else sellertype='TB';
                gettidnums();
            }
        },
        error: function(msp){
            geterrorinfo('getsellernick-msp',JSON.stringify(msp));
        }
    });
}

