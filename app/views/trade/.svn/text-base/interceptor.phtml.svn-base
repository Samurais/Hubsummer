function AddBlacknameBynick(type){/*添加拉黑用户名单和白名单*/
	if(vipuser==0){/*是免费用户*/
		$('#buyvip').modal('show');
		return;
	}
	if(type=='black'){
		var info="黑名单";
		var nicks=$("#bla_nicks").val();
		var cause=$("#bla_cause").val();
	}else if(type=='white'){
		var info="白名单";
		var nicks=$("#Wbla_nicks").val();
		var cause=$("#Wbla_cause").val();
	}
	if(nicks==''||nicks==null){
		layer.msg("请输入用户昵称哦~", 2, function(){});
		return;
	}else if(cause==''|| cause==''){
		layer.msg("请输入添加原因哦~", 2, function(){});
		return;
	}else{
		nicks=nicks.split('\n');
		for(var i = 0;i<nicks.length;i++)/*去掉空换行*/
		 {
		       if(nicks[i] == "" || typeof(nicks[i]) == "undefined")
		        {
		    	   nicks.splice(i,1);
		            i= i-1;
		       	}
		 }
		 for(var j=0;j<nicks.length;j++){/*去掉首尾空格*/
			 nicks[j]=nicks[j].replace(/(^\s*)|(\s*$)/g,"");
		 }
		if(nicks.length>10){
			layer.msg("一次最多添加10个哦~~~", 2, function(){});
			return;
		}
		$("#tjhmd_dialog").modal('hide');  
		var jx=layer.load(0, 0);
			$.ajax({
				url:'<?php echo APP_WEB_INDEX_ROOT;?>/iytrade/saveblaNick',
				type:'POST',
				dataType:'html',
				data:{'nicks':nicks,'reason':cause,'user_nick':user_nick,'type':type},
				timeout:10000,
				error:function(msp){
					layer.close(jx);
					layer.msg('添加失败', 1, 3);
				},
				success:function(rsp){
					layer.close(jx);
					if(type=="black"){
						$("#bla_nicks").val('');
						$("#bla_cause").val('');
					}else{
						$("#Wbla_nicks").val('');
						$("#Wbla_cause").val('');
					}
					layer.msg('成功添加'+nicks.length+'个'+info, 1, 1);
				}
			});
		}
	reshow();
}
