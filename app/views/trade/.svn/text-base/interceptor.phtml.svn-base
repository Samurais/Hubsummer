<script>
var isSon=false;/*是否子账号*/
var isauth=false;/*是否授权*/
var mydata='off';
var dstatus='';
window.onload = function(){
	if(dstatus==''){
		dstatus='set';
	}
	$("#cpsljsel").attr("class","dqzt");
	onload_viptime('interceptor');
	setTimeout(getUsernick,100);
	window.localStorage.setItem(user_nick+'newuser2_defen','yes');
	$('#defenratexx').empty();
	QN.initTheme();
	if(vipuser==0){
		ittoolspam('AD20140730133000','cpljpf');
	}else{
		ittoolspam('AD20140730133000','cpljpvip');
	}
} 
function getUsernick(){	/*获取登录用户，载入*/
	 QN.application.invoke({
		 cmd: "getLoginuser",
		 success: function (e){
			 if(typeof  e=== 'string'){
		          e = JSON.parse(e);
		      }
				if(decodeURI(e.sub_user_nick)==''){
					user_nick=decodeURI(e.user_nick);
				}else{
					user_nick=decodeURI(e.sub_user_nick);
					if(user_nick.indexOf(':')>0)/*是否为子账号*/
					{
						user_nick=user_nick.split(':')[0];
						isSon=true;
				    }
				}
				 $.ajax({
				        type: "POST",
					    url: '<?php echo APP_WEB_INDEX_ROOT;?>/iytrade/zdrate',/*获取授权信息*/
					    dataType:'html',
					    data:{'nick':user_nick},
					    timeout:1000,
					    success: function(data){
					       	isauth=true;
						    mydata=data;
						},
					    error: function(){
						}
				    });
				 The_loads_data();
			},/*END---->sucess**/
			error:function(msp) {
				geterrorinfo("getLoginuser",msp);
			}
		});	
}
function The_loads_data(){
	dstatus='set';
	$.ajax({
		url:'<?php echo APP_WEB_INDEX_ROOT;?>/iytrade/getdefenseSet',
		type: 'POST',
	    dataType: 'html',
	    data:{usernick:user_nick},
